platform :ios, '11.2'
source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!
target 'Travel' do
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'EasyToast'
  pod 'Cosmos', '~> 15.0'
  pod 'SwiftSpinner'
end
